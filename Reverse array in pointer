#include <stdio.h>
int main() {
    int arr[] = {10, 20, 30, 40, 50};  
    int n = sizeof(arr) / sizeof(arr[0]);  
    int *ptr = arr + n - 1;  
 printf("Array in reverse order:\n");
    for (int i = 0; i < n; i++) {
        printf("%d ", *ptr);  
        ptr--;  
        }
printf("\n");
    return 0;
}
