#include<stdio.h>
int main(){
	int a[100],n;
	printf("enter the number of elements");
	scanf("%d",&n);
	printf("enter the elements :");
	for(int i=0; i<n;i++){
	   scanf("%d",&a[i]);
	}
    int flag=0;
	for(int i=0;i<n;i++){
		int count=0;
		for(int j=0;j<n;j++){
			if(a[i]==a[j]){
				count++;
			}
			}
		if(count==1){
			flag++;}
				}
		printf("the no of non repeted elements are "
		       "%d",flag);
	return 0;
	}
