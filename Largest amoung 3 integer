#include<stdio.h>
int main(){
	int n1,n2,n3;
	printf("enter the first integer");
	scanf("%d",&n1);
	printf("enter the second integer");
	scanf("%d",&n2);
	printf("enter the third integer");
	scanf("%d",&n3);
	if(n1>n2>n3){
		printf("the greatest integer is %d",n1);
	} else if(n2>n3>n3){
		printf("the greatest integer is %d",n2);
	}else{
		printf("the greatest integer is %d",n3);
	}
	return 0;
}
