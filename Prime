#include <stdio.h>
  int main() {
    int n,flag=0;
    printf("Enter the number :");
    scanf("%d",&n);
    for(int i=2;i<n-1;i++){
        if(n%i==0){
            flag=1;
            break;
        }
    }
    if(flag==1)printf("the number is  composite");
    else printf("the number is prime");

    return 0;
}
